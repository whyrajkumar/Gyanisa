                }
              
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <h2 class="h5 mb-4 text-black">Trading Now</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-12  block-13">
                <div class="owl-carousel nonloop-block-13">
                    @foreach (var item in Model.Take(8))
                    {
